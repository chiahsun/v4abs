    if (A[6][0] > A[6][1]) begin
        A[7][0] = A[6][1];
        A[7][1] = A[6][0];
    end
    else begin
        A[7][0] = A[6][0];
        A[7][1] = A[6][1];
    end
    if (A[6][2] > A[6][3]) begin
        A[7][2] = A[6][3];
        A[7][3] = A[6][2];
    end
    else begin
        A[7][2] = A[6][2];
        A[7][3] = A[6][3];
    end
