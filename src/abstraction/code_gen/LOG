#ifndef CTE_H
#define CTE_H

#include <systemc.h>


struct YUV_TO_RGB {


    // Parameters
    const static sc_uint<4> S_U = 0;
    const static sc_uint<4> S_Y1 = 1;
    const static sc_uint<4> S_V = 2;
    const static sc_uint<4> S_BUSY1 = 3;
    const static sc_uint<4> S_Y2 = 4;
    const static sc_uint<4> S_BUSY2 = 5;

    // Registers
    sc_uint<4> state;
    sc_uint<4> state_next;
    sc_uint<8> reg_U;
    sc_uint<8> reg_Y1;
    sc_uint<8> reg_V;
    sc_uint<8> reg_Y2;

    // Wires
    sc_uint<8> reg_U_next;
    sc_uint<8> reg_Y1_next;
    sc_uint<8> reg_V_next;
    sc_uint<8> reg_Y2_next;
    sc_uint<16> R_ext;
    sc_uint<16> G_ext;
    sc_uint<16> B_ext;
    sc_uint<16> U_ext;
    sc_uint<16> Y_ext;
    sc_uint<16> V_ext;
    sc_uint<8> R_out;
    sc_uint<8> G_out;
    sc_uint<8> B_out;

    // Module instances
};

struct CTE {

    void register_data_pointer(
      sc_uint<8> * pData_yuv_in
    , sc_uint<24> * pData_rgb_out
    );


    // Inputs 
    bool clk;
    bool reset;
    bool op_mode;
    bool in_en;
    sc_uint<8> yuv_in;
    sc_uint<24> rgb_in;

    // Outputs 
    bool busy;
    bool out_valid;
    sc_uint<24> rgb_out;
    sc_uint<8> yuv_out;

    // Inouts 

    // Parameters

    // Registers

    // Wires
    bool yuv_to_rgb_in_en;
    bool yuv_to_rgb_busy;
    bool yuv_to_rgb_out_valid;

    // Module instances
    YUV_TO_RGB yuv_to_rgb_inst0;

private:


    // Read transactions
    void read_yuv_in();

    // Write transactions
    void write_rgb_out();

    // Function calls
    void function_number_0(); // reset
    void function_number_1(); // !(reset)
    void function_number_2(); // (in_en&&!(reset))
    void function_number_3(); // (!in_en&&!(reset))
    void function_number_4(); // (!in_en||reset)
    void function_number_5(); // !(busy)
    void function_number_6(); // busy
    void function_number_7(); // false

    // Data pointers
    //Input data pointers
    sc_uint<8>  _pData_yuv_in;
    //Output data pointers
    sc_uint<24>  _pData_rgb_out;
};

enum ProtocolState {
    ProtocolState_s0 // s0
  , ProtocolState_s1 // s1
  , ProtocolState_s2 // s2
  , ProtocolState_s3 // s3
  , ProtocolState_s4 // assert(0)
  , ProtocolState_s5 // s4
  , ProtocolState_s6 // s5
  , ProtocolState_s7 // s7
  , ProtocolState_s8 // s8
  , ProtocolState_s9 // s10
};

enum ProtocolEvent {
    ProtocolEvent5 // !(busy)
  , ProtocolEvent1 // !(reset)
  , ProtocolEvent3 // (!in_en&&!(reset))
  , ProtocolEvent4 // (!in_en||reset)
  , ProtocolEvent2 // (in_en&&!(reset))
  , ProtocolEvent6 // busy
  , ProtocolEvent7 // false
  , ProtocolEvent0 // reset
};

#endif // CTE_H



#include "CTE.h"

void CTE::register_data_pointer(
      sc_uint<8> * pData_yuv_in
    , sc_uint<24> * pData_rgb_out
    ) {
    _pData_yuv_in = pData_yuv_in;
    _pData_rgb_out = pData_rgb_out;
}

void CTE::run(ProtocolEvent e) {

    switch(_protocolState) {
        case ProtocolState_s0/*s0*/:
S0:
            if (e == ProtocolEvent0/*reset*/) {
                function_number_0();
                _protocolState = ProtocolState_s1/*s1*/;
                break;
            };
            if (e == ProtocolEvent1/*!(reset)*/) {
                function_number_1();
                _protocolState = ProtocolState_s0/*s0*/;
                break;
            };
            LOG_ERROR("No Such branch");
            break;
        case ProtocolState_s1/*s1*/:
S1:
            if (e == ProtocolEvent0/*reset*/) {
                function_number_0();
                _protocolState = ProtocolState_s1/*s1*/;
                break;
            };
            if (e == ProtocolEvent1/*!(reset)*/) {
                function_number_1();
                _protocolState = ProtocolState_s2/*s2*/;
                break;
            };
            LOG_ERROR("No Such branch");
            break;
        case ProtocolState_s2/*s2*/:
S2:
            if (e == ProtocolEvent2/*(in_en&&!(reset))*/) {
                read_yuv_in();
                function_number_2();
                _protocolState = ProtocolState_s3/*s3*/;
                break;
            };
            if (e == ProtocolEvent3/*(!in_en&&!(reset))*/) {
                function_number_3();
                _protocolState = ProtocolState_s2/*s2*/;
                break;
            };
            if (e == ProtocolEvent0/*reset*/) {
                function_number_0();
                _protocolState = ProtocolState_s4/*assert(0)*/;
                break;
            };
            LOG_ERROR("No Such branch");
            break;
        case ProtocolState_s3/*s3*/:
S3:
            if (e == ProtocolEvent2/*(in_en&&!(reset))*/) {
                read_yuv_in();
                function_number_2();
                _protocolState = ProtocolState_s5/*s4*/;
                break;
            };
            if (e == ProtocolEvent4/*(!in_en||reset)*/) {
                function_number_4();
                _protocolState = ProtocolState_s4/*assert(0)*/;
                break;
            };
            LOG_ERROR("No Such branch");
            break;
        case ProtocolState_s4/*assert(0)*/:
S4:
            LOG_ERROR("No Such branch");
            break;
        case ProtocolState_s5/*s4*/:
S5:
            if (e == ProtocolEvent2/*(in_en&&!(reset))*/) {
                read_yuv_in();
                function_number_2();
                _protocolState = ProtocolState_s6/*s5*/;
                break;
            };
            if (e == ProtocolEvent4/*(!in_en||reset)*/) {
                function_number_4();
                _protocolState = ProtocolState_s4/*assert(0)*/;
                break;
            };
            LOG_ERROR("No Such branch");
            break;
        case ProtocolState_s6/*s5*/:
S6:
            if (e == ProtocolEvent5/*!(busy)*/) {
                function_number_5();
                write_rgb_out();
                _protocolState = ProtocolState_s7/*s7*/;
                break;
            };
            if (e == ProtocolEvent6/*busy*/) {
                function_number_6();
                _protocolState = ProtocolState_s6/*s5*/;
                break;
            };
            LOG_ERROR("No Such branch");
            break;
        case ProtocolState_s7/*s7*/:
S7:
            if (e == ProtocolEvent2/*(in_en&&!(reset))*/) {
                read_yuv_in();
                function_number_2();
                _protocolState = ProtocolState_s8/*s8*/;
                break;
            };
            if (e == ProtocolEvent4/*(!in_en||reset)*/) {
                function_number_4();
                _protocolState = ProtocolState_s4/*assert(0)*/;
                break;
            };
            LOG_ERROR("No Such branch");
            break;
        case ProtocolState_s8/*s8*/:
S8:
            if (e == ProtocolEvent5/*!(busy)*/) {
                function_number_5();
                write_rgb_out();
                _protocolState = ProtocolState_s9/*s10*/;
                break;
            };
            if (e == ProtocolEvent6/*busy*/) {
                function_number_6();
                _protocolState = ProtocolState_s8/*s8*/;
                break;
            };
            LOG_ERROR("No Such branch");
            break;
        case ProtocolState_s9/*s10*/:
S9:
            if (e == ProtocolEvent2/*(in_en&&!(reset))*/) {
                read_yuv_in();
                function_number_2();
                _protocolState = ProtocolState_s3/*s3*/;
                break;
            };
            if (e == ProtocolEvent0/*reset*/) {
                function_number_0();
                _protocolState = ProtocolState_s1/*s1*/;
                break;
            };
            if (e == ProtocolEvent7/*false*/) {
                function_number_7();
                _protocolState = ProtocolState_s2/*s2*/;
                break;
            };
            if (e == ProtocolEvent3/*(!in_en&&!(reset))*/) {
                function_number_3();
                _protocolState = ProtocolState_s4/*assert(0)*/;
                break;
            };
            LOG_ERROR("No Such branch");
            break;
}

// Read transactions
void CTE::read_yuv_in() {
    yuv_in = *_pData_yuv_in;
}


// Write transactions
void CTE::write_rgb_out() {
    *_pData_rgb_out = rgb_out;
}


